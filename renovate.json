{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base",
    ":prHourlyLimitNone",
    ":separateMultipleMajorReleases"
  ],
  "packageRules": [
    {
      "matchManagers": [
        "maven",
        "gradle"
      ],
      "registryUrls": [
        "https://foo"
      ]
    },
    {
      "matchPackageNames": [
        "eclipse-temurin"
      ],
      "allowedVersions": "/^(11|17)(\\.[0-9]+)?(\\.[0-9]+)?/"
    }
  ]
}