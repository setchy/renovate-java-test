{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base",
    ":prHourlyLimitNone",
    ":separateMultipleMajorReleases"
  ],
  "packageRules": [
    {
      "matchPackageNames": [
        "eclipse-temurin"
      ],
      "allowedVersions": "/^(11|17)(\\.[0-9]+)?(\\.[0-9]+)?/"
    },
    {
      "description": "Mulesoft Libraries",
      "matchPackagePatterns": [
        "^com.mulesoft",
        "^org.mule"
      ],
      "allowedVersions": "/^[0-9]+\\.[0-9]+\\.[0-9]+$/",
      "registryUrls": [
        "https://repository.mulesoft.org/releases/"
      ]
    }
  ],
  "hostRules": [
    {
      "hostType": "maven",
      "baseUrl": "https://repository.mulesoft.org/releases/"
    }
  ]
}