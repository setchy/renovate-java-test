{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base",
    ":prHourlyLimitNone",
    ":separateMultipleMajorReleases"
  ],
  "java": {
    "registryUrls": "https://foo",
  },
  "packageRules": [
    {
      "matchPackageNames": [
        "eclipse-temurin"
      ],
      "allowedVersions": "/^(11|17)(\\.[0-9]+)?(\\.[0-9]+)?/"
    }
  ]
}